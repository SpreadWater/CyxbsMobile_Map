<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginBottom="1dp"
    android:layout_width="match_parent"
    tools:ignore="ResourceName"
    android:layout_height="50dp">

    <ImageView
        android:id="@+id/map_iv_search"
        android:layout_width="19dp"
        android:layout_height="19dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="14dp"
        android:src="@drawable/map_ic_search_status"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/map_tv_result_search_place"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="7dp"
        android:paddingLeft="2dp"
        android:paddingTop="2dp"
        android:paddingRight="2dp"
        android:paddingBottom="2dp"
        android:text="四海苑5舍"
        android:textColor="@color/levelTwoFontColor"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/map_iv_search"
        app:layout_constraintLeft_toRightOf="@id/map_iv_search"
        app:layout_constraintTop_toTopOf="@id/map_iv_search" />
</androidx.constraintlayout.widget.ConstraintLayout>