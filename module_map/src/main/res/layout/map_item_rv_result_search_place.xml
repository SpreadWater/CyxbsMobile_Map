<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginBottom="1dp"
    android:layout_width="match_parent"
    tools:ignore="ResourceName"
    android:layout_height="50dp">
<ImageView
    android:id="@+id/iv_map_search"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/common_back"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="10dp"/>

    <TextView
        android:id="@+id/tv_map_result_search_place"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="四海苑5舍"
        android:textColor="@color/levelTwoFontColor"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_marginLeft="10dp"
        android:paddingTop="2dp"
        android:paddingBottom="2dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        app:layout_constraintBottom_toBottomOf="@id/iv_map_search"
        app:layout_constraintTop_toTopOf="@id/iv_map_search"
        app:layout_constraintLeft_toRightOf="@id/iv_map_search" />
</androidx.constraintlayout.widget.ConstraintLayout>