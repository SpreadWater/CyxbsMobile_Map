<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
<!--    by徐国林-->
    <style name="map_tablayout_textsize" parent="TextAppearance.AppCompat.Button" tools:ignore="ResourceName">
        <item name="android:textSize">15sp</item>
    </style>
    <style name="map_anim_popwindow" tools:ignore="ResourceName">
        <item name="android:windowEnterAnimation">@anim/map_popupwindow_show</item>
        <item name="android:windowExitAnimation">@anim/map_popupwindow_hide</item>
    </style>

    <style name="Transparent" parent="@style/AppTheme" tools:ignore="ResourceName">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

</resources>